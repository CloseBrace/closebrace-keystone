              <div>
                  <label for="drip-email">Email Address<sup style="color:#A00">*</sup>
                  <input type="email" id="drip-email" name="fields[email]" value="" /></label>
              </div>
              <div>
                  <label for="drip-first-name">First Name
                  <input type="text" id="drip-first-name" name="fields[first_name]" value="" /></label>
              </div>
            <div>
              <input type="submit" value="Sign Up" data-drip-attribute="sign-up-button" class="btn btn-primary" />
            </div>
          </fieldset>
        </form>

        <p class="tiny">
          <sup style="color:#A00">*</sup> required
        </p>

        </section>
